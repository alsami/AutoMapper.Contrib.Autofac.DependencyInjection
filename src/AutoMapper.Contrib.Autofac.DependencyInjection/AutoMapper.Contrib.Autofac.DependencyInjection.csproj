<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>5.6.0</Version>
    <FileVersion>5.6.0.0</FileVersion>
    <Copyright>2022©Sami Al Khatib</Copyright>
    <Authors>Sami Al Khatib</Authors>
    <Title>AutoMapper-Autofac-Extension</Title>
    <Description>An extension for Autofac-IoC to use AutoMapper with ease.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/alsami/AutoMapper.Contrib.Autofac.DependencyInjection.git</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/alsami/AutoMapper.Contrib.Autofac.DependencyInjection/blob/main/changelog.md</PackageReleaseNotes>
    <PackageTags>AutoMapper, Object-To-Object-Mapping, Mapper, Autofac, IoC, Dependency-Injection</PackageTags>
    <PackageIconUrl>https://s3.amazonaws.com/automapper/icon.png</PackageIconUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/alsami/AutoMapper.Contrib.Autofac.DependencyInjection.git</RepositoryUrl>
    <TreatWarningAsError>true</TreatWarningAsError>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Autofac" Version="6.3.0" />
    <PackageReference Include="AutoMapper" Version="11.0.0" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
</Project>