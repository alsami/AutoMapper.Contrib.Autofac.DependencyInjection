<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Version>5.2.0</Version>
    <FileVersion>5.2.0.0</FileVersion>
    <Copyright>2020©Sami Al Khatib</Copyright>
    <Authors>Sami Al Khatib</Authors>
    <Title>AutoMapper-Autofac-Extension</Title>
    <Description>An extension for Autofac-IoC to use AutoMapper with ease.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/alsami/AutoMapper.Contrib.Autofac.DependencyInjection.git</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/alsami/AutoMapper.Contrib.Autofac.DependencyInjection/blob/master/changelog.md</PackageReleaseNotes>
    <PackageTags>AutoMapper, Object-To-Object-Mapping, Mapper, Autofac, IoC, Dependency-Injection</PackageTags>
    <PackageIconUrl>https://s3.amazonaws.com/automapper/icon.png</PackageIconUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/alsami/AutoMapper.Contrib.Autofac.DependencyInjection.git</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Autofac" Version="6.1.0" />
    <PackageReference Include="AutoMapper" Version="10.1.1" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
</Project>